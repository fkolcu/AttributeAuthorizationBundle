{
  "name": "fkolcu/attribute-authorization-bundle",
  "description": "This bundle provides attribute to authorize user on class or method level",
  "type": "symfony-bundle",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "FK\\Bundle\\AttributeAuthorizationBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FK\\Bundle\\AttributeAuthorizationBundle\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Furkan",
      "email": "hello@furkankolcu.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "symfony/http-kernel": "^6.0",
    "symfony/dependency-injection": "^6.0",
    "symfony/config": "^6.0",
    "symfony/security-core": "^6.0",
    "lexik/jwt-authentication-bundle": "^2.16"
  },
  "require-dev": {
    "phpspec/phpspec": "^7.2"
  },
  "scripts": {
    "test": "php vendor/bin/phpspec run"
  }
}
